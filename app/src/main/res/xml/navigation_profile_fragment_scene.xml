<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lol="http://schemas.android.com/tools"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        lol:constraintSetEnd="@+id/end"
        lol:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="500">

        <KeyFrameSet />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/cvProfileImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/profile_fragment_content_margin_top"
            android:layout_marginEnd="@dimen/profile_fragment_content_margin_horizontal"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/viewChangeProfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/profile_fragment_icon_margin_top"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintStart_toStartOf="@id/tvProfileName"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileName"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/cvProfileBasket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/profile_fragment_icon_margin_start"
            android:layout_marginTop="@dimen/profile_fragment_icon_margin_top"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintStart_toEndOf="@id/viewChangeProfile"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileName"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/cvProfileOut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/profile_fragment_icon_margin_start"
            android:layout_marginTop="@dimen/profile_fragment_icon_margin_top"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintStart_toEndOf="@id/cvProfileBasket"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileName"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/cvProfileOut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/profile_fragment_icon_margin_start"
            android:layout_marginTop="@dimen/profile_fragment_icon_margin_top"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintStart_toEndOf="@id/cvProfileBasket"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileName"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/cvGraph"
            android:layout_width="@dimen/card_size"
            android:layout_height="@dimen/card_size"
            android:layout_marginStart="@dimen/real_medium_padding"
            android:layout_marginTop="@dimen/base_padding"
            android:alpha="0"
            android:visibility="visible"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/tvProfileStatistics"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/tvProfileEmailProfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintStart_toStartOf="@id/tvProfileEmail"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileEmail"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/cvProfileLogin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/profile_fragment_card_view_bottom_margin"
            android:alpha="0"
            android:rotationX="-56"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintBottom_toTopOf="@id/cvProfileRegistration"
            motion:layout_constraintEnd_toEndOf="@id/cvProfileImage"
            motion:layout_constraintStart_toStartOf="@id/tvProfileName"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileEmailProfile"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/cvProfileRegistration"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:rotationX="-56"
            android:scaleX="0"
            android:scaleY="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="@+id/cvProfileLogin"
            motion:layout_constraintStart_toStartOf="@+id/cvProfileLogin"
            motion:layout_constraintTop_toBottomOf="@id/cvProfileLogin"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/tvProfileName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/tvProfileHello"
            motion:layout_constraintTop_toBottomOf="@id/tvProfileHello"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/tvProfileEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/tvProfileStatistics"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:visibilityMode="ignore">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint android:id="@+id/gProfileNotEnter">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/cvGraph">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/viewChangeProfile">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/cvProfileBasket">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/cvProfileOut">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/gProfileEmail">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/tvProfileStatistics">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/tvProfileEmail">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/tvProfileEmailProfile">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/gProfileButton">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/cvProfileLogin">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/cvProfileRegistration">
            <PropertySet lol:visibilityMode="ignore" />
        </Constraint>
    </ConstraintSet>
</MotionScene>